# Source: https://github.com/crosstool-ng/crosstool-ng/blob/master/samples/armv6-rpi-linux-gnueabi/crosstool.config

CT_CONFIG_VERSION="3"
CT_ARCH_ARM=y
CT_ARCH_CPU="arm1176jzf-s"
CT_ARCH_SUFFIX="v6"
CT_ARCH_FPU="vfp"
CT_TARGET_VENDOR="rpi"

CT_KERNEL_LINUX=y
CT_LINUX_V_4_19=y
CT_LINUX_VERSION="4.19.105"

CT_GLIBC_V_2_28=y
CT_GLIBC_VERSION="2.28"
CT_GLIBC_LOCALES=y
CT_GLIBC_KERNEL_VERSION_CHOSEN=y
CT_GLIBC_MIN_KERNEL="4.19.105"

CT_GCC_V_8=y
CT_GCC_VERSION="8.3.0"
CT_CC_GCC_DISABLE_PCH=y
CT_CC_GCC_BUILD_ID=y
CT_CC_GCC_LNK_HASH_STYLE_BOTH=y
CT_CC_LANG_CXX=y

CT_BINUTILS_LINKER_LD_GOLD=y
CT_BINUTILS_GOLD_THREADS=y
CT_BINUTILS_LD_WRAPPER=y
CT_BINUTILS_PLUGINS=y
CT_BINUTILS_V_2_31=y
CT_BINUTILS_VERSION="2.31.1"

CT_DEBUG_GDB=y

# CT_GDB_CROSS_PYTHON is not set
